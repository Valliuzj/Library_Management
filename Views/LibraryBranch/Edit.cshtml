@model LibraryManagement.Models.LibraryBranch

<h1> Edit Library Branch </h1>

<form asp-controller="LibraryBranch" asp-action="Edit" method="post">
    <input type="hidden" asp-for="LibraryBranchId" />

    <div class="form-group">
        <label asp-for="BranchName"></label>
        <input asp-for="BranchName" class="form-control" />
        <span asp-validation-for="BranchName" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>